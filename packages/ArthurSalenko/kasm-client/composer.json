{
    "name": "arthur-salenko/kasm-client",
    "description": "Laravel wrapper for the Kasm Workspaces API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ArthurSalenko\\KasmClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArthurSalenko\\KasmClient\\KasmServiceProvider"
            ],
            "aliases": {
                "Kasm": "ArthurSalenko\\KasmClient\\Facades\\Kasm"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
